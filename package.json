{
    "license": "ISC",
    "bin": {"quarkus-run": "jdeploy-bundle/jdeploy.js"},
    "name": "jdeploy-service-example",
    "description": "A simple quarkus rest app deployed using jDeploy",
    "files": ["jdeploy-bundle"],
    "main": "index.js",
    "preferGlobal": true,
    "scripts": {"test": "echo \"Error: no test specified\" && exit 1"},
    "version": "1.0.0",
    "jdeploy": {
        "singleton": true,
        "fallbackToUniversal": false,
        "generateLegacyBundles": false,
        "helper": {"actions": [{
            "description": "Open the Settings Panel",
            "label": "Open Settings",
            "url": "helloquarkus://settings"
        }]},
        "publishTargets": [],
        "javaVersion": "21",
        "urlSchemes": ["helloquarkus"],
        "jar": "build/jdeploy-service-example-1.0.0-SNAPSHOT-runner.jar",
        "title": "Quarkus Run",
        "downloadPage": {"platforms": ["all"]},
        "platformBundlesEnabled": false,
        "commands": {"hello-quarkus": {
            "implements": [
                "updater",
                "service_controller"
            ],
            "description": "A service example"
        }}
    },
    "dependencies": {
        "command-exists-promise": "^2.0.2",
        "node-fetch": "2.6.7",
        "tar": "^4.4.8",
        "yauzl": "^2.10.0",
        "shelljs": "^0.8.4"
    }
}