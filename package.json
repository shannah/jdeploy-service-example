{
    "bin": {"quarkus-run": "jdeploy-bundle/jdeploy.js"},
    "description": "A simple quarkus rest app deployed using jDeploy",
    "main": "index.js",
    "preferGlobal": true,
    "version": "1.0.0",
    "jdeploy": {
        "fallbackToUniversal": false,
        "generateLegacyBundles": false,
        "publishTargets": [],
        "javaVersion": "21",
        "jar": "build/quarkus-app/quarkus-run.jar",
        "title": "Quarkus Run",
        "downloadPage": {"platforms": ["default"]},
        "platformBundlesEnabled": false,
        "commands": {"hello-quarkus": {
            "implements": [
                "updater",
                "service_controller"
            ],
            "description": "A service example"
        }}
    },
    "dependencies": {
        "command-exists-promise": "^2.0.2",
        "node-fetch": "2.6.7",
        "tar": "^4.4.8",
        "yauzl": "^2.10.0",
        "shelljs": "^0.8.4"
    },
    "license": "ISC",
    "name": "jdeploy-service-example",
    "files": ["jdeploy-bundle"],
    "scripts": {"test": "echo \"Error: no test specified\" && exit 1"}
}